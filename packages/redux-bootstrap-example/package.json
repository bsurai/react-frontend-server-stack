    "@types/node": "^7.0.12",
    "@types/node-sass": "^3.10.32",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^3.0.9",
    "@types/react-router-redux": "^4.0.43",
