    "@accounts/rest-client": "0.0.3",
    "@accounts/rest-express": "0.0.3",
    "@accounts/server": "0.0.4",
    "@redux-bootstrap/bootstrap": "^0.0.1",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.16.0",
    "node-sass": "^4.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.3",
    "react-router-redux": "^4.0.8",
